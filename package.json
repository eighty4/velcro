  "keywords": [
    "elasticsearch",
    "development",
    "migration",
    "data",
    "devtool",
    "automation",
    "index",
    "mapping",
    "document"
  ],
  "version": "0.0.7-0",
  "license": "BSD-2-Clause",
  "main": "lib/velcro.exports.js",
