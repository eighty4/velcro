  "keywords": [
    "elasticsearch",
    "development",
    "migration",
    "data",
    "devtool",
    "automation",
    "index",
    "mapping",
    "document"
  ],
  "version": "0.0.7-4",
  "license": "BSD-2-Clause",
  "main": "lib/velcro.exports.js",
  "types": "lib/velcro.exports.d.ts",
